{
  "name": "@nahisaho/satori",
  "version": "0.27.0",
  "description": "SATORI â€” Agent Skills for Science. GitHub Copilot Agent Skills collection for scientific data analysis.",
  "main": "index.js",
  "bin": {
    "satori": "bin/satori.js"
  },
  "files": [
    "bin/",
    "src/.github/"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit/",
    "test:validation": "vitest run tests/validation/",
    "test:coverage": "vitest run --coverage",
    "validate:skills": "vitest run tests/validation/skill-format.test.js",
    "validate:tu": "vitest run tests/validation/tu-coverage.test.js",
    "lint": "biome check bin/ tests/ vitest.config.js",
    "lint:fix": "biome check --write bin/ tests/ vitest.config.js",
    "format": "biome format --write bin/ tests/ vitest.config.js",
    "prepare": "husky"
  },
  "keywords": [
    "copilot",
    "agent-skills",
    "science",
    "data-analysis",
    "github-copilot"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nahisaho/satori.git"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "bin/**/*.js": [
      "biome check --write"
    ],
    "tests/**/*.js": [
      "biome check --write"
    ],
    "vitest.config.js": [
      "biome check --write"
    ]
  }
}
